    "# 데이터 선택 (기본)\n",
    "import pandas as pd\n",
    "df = pd.read_csv('score.csv', index_col='지원번호')\n",
    "\n",
    "# column 선택\n",
    "df['이름']\n",
    "\n",
    "# column 선택 (정수 index)\n",
    "df.columns[0]\n",
    "for i in range(1,5):\n",
    "    print(df.columns[i])\n",
    "\n",
    "df[df.columns[0]]\n",
    "df[df.columns[-1]] # 맨 끝에 있는 데이터를 불러오기 \n",
    "\n",
    "df['영어'][0:5]\n",
    "df[['이름','키']][:3]\n"
